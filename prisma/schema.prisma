datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id         Int      @id @default(autoincrement())
  name       String
  email      String
  friendName String
  friendEmail String
  message    String?
  createdAt  DateTime @default(now())
}